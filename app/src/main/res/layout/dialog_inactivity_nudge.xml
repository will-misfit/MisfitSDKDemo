<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.misfit.misfitsdkdemo.view.RangePreference
        android:id="@+id/prf_nudge_start_hour"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="16dp"
        app:range_max="23"
        app:range_min="0"
        app:title_text="start_hour" />

    <com.misfit.misfitsdkdemo.view.RangePreference
        android:id="@+id/prf_nudge_start_min"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="16dp"
        app:range_max="59"
        app:range_min="0"
        app:title_text="start_min  " />

    <com.misfit.misfitsdkdemo.view.RangePreference
        android:id="@+id/prf_nudge_end_hour"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="16dp"
        app:range_max="23"
        app:range_min="0"
        app:title_text="end_hour  " />

    <com.misfit.misfitsdkdemo.view.RangePreference
        android:id="@+id/prf_nudge_end_min"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="16dp"
        app:range_max="59"
        app:range_min="0"
        app:title_text="end_min    " />

    <com.misfit.misfitsdkdemo.view.RangePreference
        android:id="@+id/prf_nudge_repeat_min"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="16dp"
        app:range_max="59"
        app:range_min="0"
        app:title_text="repeat_min" />
</LinearLayout>